package losrebellos.lospistoleros.vo {	/*	 *	 * @author los rebellos	 *	 */	public class URLVO 	{		/*		 * 		 * VARIABLES		 * 		 */		public var url:String;		public var data:Object;						/*		 * 		 * CONSTRUCTOR		 * 		 */		public function URLVO(_url:String, _data:Object)		{			url = _url;			data = _data;		}						/*		 * 		 * DATA		 * 		 */		public function getData(name:String):String		{			var value:String = data[name];			if(value)				return value;						return "";		}	}}
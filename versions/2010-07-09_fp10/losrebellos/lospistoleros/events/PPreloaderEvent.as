package losrebellos.lospistoleros.events {	import losrebellos.events.EventPlus;	import flash.events.Event;	/*	 *	 * @author los rebellos	 *	 */	public class PPreloaderEvent extends EventPlus 	{		/*		 * 		 * EVENTS		 * 		 */		public static const PROGRESS:String = "pistoleros/preloader/progress";		public static const COMPLETE:String = "pistoleros/preloader/complete";		public static const ERROR:String = "pistoleros/preloader/error";						/*		 * 		 * ITEMS		 * 		 */		public static const CONTENT_COMPLETE:String = "pistoleros/preloader/content/complete";		public static const FONTS_COMPLETE:String = "pistoleros/preloader/fonts/complete";		public static const CSS_COMPLETE:String = "pistoleros/preloader/css/complete";				//data		public var percent_loaded:Number = 0;						/*		 * 		 * CONSTRUCTOR		 * 		 */		public function PPreloaderEvent(type:String, _percent_loaded:Number = 0, bubbles : Boolean = false, cancelable : Boolean = false)		{			percent_loaded = _percent_loaded;			super(type, bubbles, cancelable);		}						/*		 * 		 * UTILS		 * 		 */		override public function clone():Event		{			return new PPreloaderEvent(this.type, this.percent_loaded, this.bubbles, this.cancelable);		}	}}
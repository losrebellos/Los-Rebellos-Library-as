package losrebellos.lospistoleros.components.preloader.sprite {	import losrebellos.lospistoleros.core.controller.sprite.PControllerSprite;	import losrebellos.lospistoleros.core.model.sprite.IPModelSprite;	import losrebellos.lospistoleros.core.view.sprite.IPViewSprite;	/*	 *	 * @author los rebellos	 *	 */	public class PPreloaderControllerSprite extends PControllerSprite	{		/*		 * 		 * CONSTRUCTOR		 * 		 */		public function PPreloaderControllerSprite(m:IPModelSprite, v:IPViewSprite = null, name:String = null)		{			super(m, v, name);		}						/*		 * 		 * LOAD		 * 		 */		public function load():void		{			(_model as PPreloaderModelSprite).load();		}						/*		 * 		 * LANGUAGE		 * 		 */		public function setLanguage(_language:String):void		{			(_model as PPreloaderModelSprite).setLanguage(_language);		}	}}
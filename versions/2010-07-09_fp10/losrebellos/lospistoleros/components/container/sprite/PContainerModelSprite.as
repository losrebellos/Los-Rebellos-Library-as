package losrebellos.lospistoleros.components.container.sprite {	import asual.swfaddress.SWFAddress;	import asual.swfaddress.SWFAddressEvent;	import losrebellos.lospistoleros.core.model.sprite.PModelSprite;	import losrebellos.lospistoleros.events.PNavigateEvent;	/*	 *	 * @author los rebellos	 *	 */	public class PContainerModelSprite extends PModelSprite	{		/*		 * 		 * VARIABLES		 * 		 */		protected var _url:String = "";						/*		 * 		 * CONSTRUCTOR		 * 		 */		public function PContainerModelSprite(name:String = null)		{			super(name);						SWFAddress.addEventListener(SWFAddressEvent.CHANGE, swfAddressChange);		}						/*		 * 		 * FROM BROWSER HISTORY		 * 		 */		public function initialize():void		{			swfAddressChange();		}		protected function swfAddressChange(e:SWFAddressEvent = null):void		{			_url = SWFAddress.getValue();						this.dispatchEvent(new PNavigateEvent(PNavigateEvent.CHANGED, _url));		}						/*		 * 		 * FROM FLASH SITE		 * 		 */		public function setURL(value:String):void		{			SWFAddress.setValue(value);		}		public function getURL():String		{			return _url;		}	}}
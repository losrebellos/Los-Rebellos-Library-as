package losrebellos.color {	/**	 * 	 * @author los rebellos	 * 	 */	public class RGB extends Object	{		/*		 *		 * CONSTANTS		 *		 */		public static var GREY_SCALE:RGB = new RGB(0.212671, 0.715160, 0.072169);		public static var BLACK_AND_WHITE:RGB = new RGB(0.212671 * 256, 0.715160 * 256, 0.072169 * 256);						/*		 *		 * VARIABLES		 *		 */		protected var _red:Number = 1;		public function set red(value:Number):void		{			_red = value;		}		public function get red():Number		{			return _red;		}		protected var _green:Number = 1;		public function set green(value:Number):void		{			_green = value;		}		public function get green():Number		{			return _green;		}		protected var _blue:Number = 1;		public function set blue(value:Number):void		{			_blue = value;		}		public function get blue():Number		{			return _blue;		}				/*		 *		 * CONSTRUCTOR		 *		 */		public function RGB(red:Number = 1, green:Number = 1, blue:Number = 1) 		{			super();						_red = red;			_green = green;			_blue = blue;		}		public function parse(color:uint):void		{			_red = (color >> 16 & 0xFF) / 255;			_green = (color >> 8  & 0xFF) / 255;			_blue = (color & 0xFF) / 255;		}	}}
package losrebellos.events {	import flash.events.Event;	/*	 *	 * @author los rebellos	 *	 */	public class EventPlus extends Event implements IEventPlus	{		/*		 * 		 * CONSTRUCTOR		 * 		 */		public function EventPlus(type:String, bubbles:Boolean = false, cancelable:Boolean = false)		{			super(type, bubbles, cancelable);		}						/*		 * 		 * UTILS		 * 		 */		public function stopAllPropagations():void		{			this.stopPropagation();			this.stopImmediatePropagation();		}	}}
package losrebellos.rendering {	/*	 *	 * @author los rebellos	 *	 */	public class RenderType 	{		/*		 * 		 * TYPES		 * 		 */		public static const NORMAL:String = "renderType/normal";		public static const BITMAP:String = "renderType/bitmap";						/*		 * 		 * VARIABLES		 * 		 */		public var bitmapDataRenderMode:BitmapDataRenderType = new BitmapDataRenderType;		public var bitmapRenderMode:BitmapRenderType = new BitmapRenderType;						/*		 * 		 * CONSTRUCTOR		 * 		 */		public function RenderType(_bitmapDataRenderMode:BitmapDataRenderType = null, _bitmapRenderMode:BitmapRenderType = null)		{			if(!_bitmapDataRenderMode)				bitmapDataRenderMode = BitmapDataRenderType.DEFAULT;			else				bitmapDataRenderMode = _bitmapDataRenderMode;						if(!_bitmapRenderMode)				bitmapRenderMode = BitmapRenderType.DEFAULT;			else				bitmapRenderMode = _bitmapRenderMode;		}	}}
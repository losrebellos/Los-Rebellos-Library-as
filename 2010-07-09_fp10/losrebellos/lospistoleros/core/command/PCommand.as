package losrebellos.lospistoleros.core.command {	import losrebellos.lospistoleros.core.accessor.PAccessor;	import flash.errors.IllegalOperationError;	/*	 *	 * @author los rebellos	 *	 */	public class PCommand extends PAccessor implements IPCommand 	{		/*		 * 		 * CONSTRUCTOR		 * 		 */		public function PCommand(name:String = null, type:String = null)		{			super(name, type);		}				/*		 * 		 * EXECUTION		 * 		 */		public function execute():Boolean		{			throw new IllegalOperationError("PCommand.execute need to be overridden");						return false;		}	}}
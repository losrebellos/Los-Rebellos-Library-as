package losrebellos.lospistoleros.events {	import losrebellos.events.EventPlus;	import flash.events.Event;	/*	 *	 * @author los rebellos	 *	 */	public class PNavigateEvent extends EventPlus 	{		/*		 * 		 * EVENTS		 * 		 */		public static const CHANGE:String = "pistoleros/navigate/change";		public static const CHANGED:String = "pistoleros/navigate/changed";				//data		public var url:String = "";		public var url_hashed:Array = [];						/*		 * 		 * CONSTRUCTOR		 * 		 */		public function PNavigateEvent(type:String, _url:String = "")		{			url = _url;			url_hashed = _url.split("/");						super(type, true);		}						/*		 * 		 * UTILS		 * 		 */		override public function clone():Event		{			return new PNavigateEvent(this.type, this.url);		}	}}
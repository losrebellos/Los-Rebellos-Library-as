package losrebellos.utils {	import losrebellos.console.Console;	/**	 * @author los rebellos	 */	public class ObjectUtils {				/*		 * 		 * PRINT AN OBJECT		 * 		 * return a string of the elements and their values		 * 		 */		public static function print(obj:*, spaces:String = " // "):String 		{			var s:String = obj;			for (var i:String in obj)			{			       s += spaces + i + ": "; 			       s += print(obj[i], spaces + "\t");			}			return s;		}						/*		 * 		 * TRACE AN OBJECT		 * 		 */		public static function traceObject(obj:Object):void		{			for (var i:String in obj)				Console.output("ObjectUtils", i + ": " + obj[i]);		}						/*		 * 		 * CHECK EXISTENCE OF A PARAMETER IN AN OBJECT		 * 		 */		public static function hasParam(obj:Object,param:String,_return_true:* = true,_return_false:* = false):*		{			if(obj[param])				return _return_true;			else				return _return_false;		}						/*		 * object is empty?		 * 		 * return true if empty, false if not		 */		public static function isEmpty(obj:Object):Boolean		{			for (var i:String in obj)				if(obj[i])					return false;			return true;		}						/*		 * length of an object		 * 		 * return the length of an object		 */		public static function length(obj:*):int 		{			var count:int = 0;			for (var i:String in obj)				if(obj[i])					count++;			return count;		}						/*		 * set an object		 */		public static function setVar(_obj:Object, _name:String, _value:* = null):void		{			_obj[_name] = _value;		}	}}
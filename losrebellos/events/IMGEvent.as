package losrebellos.events {	import flash.events.Event;			/*	 *	 * @author los rebellos	 *	 */	public class IMGEvent extends EventPlus	{		/*		 * 		 * EVENTS		 * 		 */		public static const PROGRESS:String = "IMGEvent/progress";		public static const COMPLETE:String = "IMGEvent/complete";		public static const ERROR:String = "IMGEvent/error";				//data		public var data:*;						/*		 * 		 * COSNTRUCTOR		 * 		 */		public function IMGEvent(type:String, _data:* = null)		{			data = _data;						super(type);		}						/*		 * 		 * UTILS		 * 		 */		override public function clone():Event		{			return new IMGEvent(this.type, this.data);		}	}}
package losrebellos.lospistoleros.core.model {	import losrebellos.events.EventDispatcherPlus;	import losrebellos.lospistoleros.core.controller.IPController;	import losrebellos.lospistoleros.core.view.IPView;	/*	 *	 * @author los rebellos	 *	 */	public class PModel extends EventDispatcherPlus implements IPModel 	{		/*		 * 		 * VARIABLES		 * 		 */		protected var view:IPView;		protected var controller:IPController;				/*		 * 		 * MVC		 * 		 */		public function PModel()		{			super();		}		public function setView(v:IPView):void		{			view = v;		}		public function getView():IPView		{			return view;		}		public function setController(c:IPController):void		{			controller = c;		}		public function getController():IPController		{			return controller;		}	}}
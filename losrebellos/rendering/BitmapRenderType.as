package losrebellos.rendering {	import flash.display.PixelSnapping;	/*	 *	 * @author los rebellos	 *	 */	public class BitmapRenderType 	{		/*		 * 		 * DEFAULT		 * 		 */		public static var DEFAULT:BitmapRenderType = new BitmapRenderType;						/*		 * 		 * VARIABLES		 * 		 */		public var pixelSnapping:String = PixelSnapping.AUTO;		public var smoothing:Boolean = true;						/*		 * 		 * CONSTRUCTOR		 * 		 */		public function BitmapRenderType(_pixelSnapping:String = "auto", _smoothing:Boolean = true)		{			pixelSnapping = _pixelSnapping;			smoothing = _smoothing;		}	}}
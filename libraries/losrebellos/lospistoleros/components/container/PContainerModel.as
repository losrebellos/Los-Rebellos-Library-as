package losrebellos.lospistoleros.components.container {	import asual.swfaddress.SWFAddress;	import asual.swfaddress.SWFAddressEvent;	import losrebellos.lospistoleros.core.model.PModel;	import losrebellos.lospistoleros.events.PNavigateEvent;	/*	 *	 * @author los rebellos	 *	 */	public class PContainerModel extends PModel 	{		/*		 * 		 * VARIABLES		 * 		 */		protected var url:String = "";						/*		 * 		 * CONSTRUCTOR		 * 		 */		public function PContainerModel()		{			super();						SWFAddress.addEventListener(SWFAddressEvent.CHANGE, swfAddressChange);		}						/*		 * 		 * FROM BROWSER HISTORY		 * 		 */		public function initialize():void		{			swfAddressChange();		}		protected function swfAddressChange(e:SWFAddressEvent = null):void		{			url = SWFAddress.getValue();						this.dispatchEvent(new PNavigateEvent(PNavigateEvent.CHANGED, url));		}						/*		 * 		 * FROM FLASH SITE		 * 		 */		public function setURL(value:String):void		{			SWFAddress.setValue(value);		}		public function getURL():String		{			return url;		}	}}
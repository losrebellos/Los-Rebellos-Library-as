package losrebellos.lospistoleros.core.command {	import losrebellos.events.EventDispatcherPlus;	import losrebellos.lospistoleros.core.dispatcher.PDispatcher;	import flash.errors.IllegalOperationError;	/*	 *	 * @author los rebellos	 *	 */	public class PCommand extends EventDispatcherPlus implements IPCommand 	{		/*		 * 		 * VARIABLES		 * 		 */		protected var _NAME:String = null;						/*		 * 		 * CONSTRUCTOR		 * 		 */		public function PCommand(name:String = null)		{			super();						_NAME = name;						PDispatcher.instance.registerCommand(this);		}				/*		 * 		 * EXECUTION		 * 		 */		public function execute():Boolean		{			throw new IllegalOperationError("PCommand.execute need to be extends");						return false;		}						/*		 * 		 * NAME		 * 		 */		public function set NAME(value:String):void		{			_NAME = value;		}		public function get NAME():String		{			return _NAME;		}	}}
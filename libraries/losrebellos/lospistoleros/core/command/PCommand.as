package losrebellos.lospistoleros.core.command {	import losrebellos.events.EventDispatcherPlus;	import flash.errors.IllegalOperationError;	/*	 *	 * @author los rebellos	 *	 */	public class PCommand extends EventDispatcherPlus implements IPCommand 	{		/*		 * 		 * CONSTRUCTOR		 * 		 */		public function PCommand(name:String = null, type:String = null)		{			super();						_NAME = name;			_TYPE = type;		}				/*		 * 		 * EXECUTION		 * 		 */		public function execute():Boolean		{			throw new IllegalOperationError("PCommand.execute need to be overridden");						return false;		}						/*		 * 		 * NAME		 * 		 */		protected var _NAME:String = null;		public function set NAME(value:String):void		{			_NAME = value;		}		public function get NAME():String		{			return _NAME;		}						/*		 * 		 * TYPE		 * 		 */		protected var _TYPE:String = null;		public function set TYPE(value:String):void		{			_TYPE = value;		}		public function get TYPE():String		{			return _TYPE;		}	}}
package losrebellos.data {	import losrebellos.events.EventDispatcherPlus;	/*	 *	 * @author los rebellos	 *	 */	public class AbstractXMLData extends EventDispatcherPlus implements IAbstractXMLData	{		/*		 * 		 * VARIABLES		 * 		 */		protected var _xml:XML;						/*		 * 		 * CONSTRUCTOR		 * 		 */		public function AbstractXMLData()		{			super();		}						/*		 * 		 * XML		 * 		 */		public function setXML(xml:XML):void		{			_xml = xml;		}		public function getXML():XML		{			return _xml;		}						/*		 * 		 * UTILS		 * 		 */		public function empty():void		{			_xml = null;		}	}}
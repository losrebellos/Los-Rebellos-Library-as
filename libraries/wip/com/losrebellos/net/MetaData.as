package com.losrebellos.net {	/*	 *	 * @author los rebellos	 *	 */	public final class MetaData extends Object 	{		////////////////////////////////////////////////////////////////////////////////////////////////////////////////
		// VARIABLES
		////////////////////////////////////////////////////////////////////////////////////////////////////////////////		public var duration:Number = 0;		public var framerate:int = 0;		public var width:Number = 0;		public var height:Number = 0;						////////////////////////////////////////////////////////////////////////////////////////////////////////////////
		// CONSTRUCTOR
		////////////////////////////////////////////////////////////////////////////////////////////////////////////////		public function MetaData(object:Object = null)		{			if(object)			{				duration = object["duration"];				framerate = object["framerate"];				width = object["width"];				height = object["height"];			}		}						////////////////////////////////////////////////////////////////////////////////////////////////////////////////
		// UTILS
		////////////////////////////////////////////////////////////////////////////////////////////////////////////////		public function toString():String		{			return "Metadata:\n" + "\t- duration: " + duration + "\n" + "\t- framerate: " + framerate + "\n" + "\t- width: " + width + "\n" + "\t- height: " + height;		}	}}
package com.losrebellos.events {	import flash.events.Event;	/*	 *	 * @author los rebellos	 *	 */	public final class NetStreamClientEvent extends EventPlus	{		////////////////////////////////////////////////////////////////////////////////////////////////////////////////
		// CONSTANTS
		////////////////////////////////////////////////////////////////////////////////////////////////////////////////		public static const META_DATA:String		= "NetStreamClientEvent/metaData";		public static const CUE_POINT:String		= "NetStreamClientEvent/cuePoint";		public static const IMAGE_DATA:String		= "NetStreamClientEvent/imageData";		public static const PLAY_STATUS:String		= "NetStreamClientEvent/playStatus";		public static const TEXT_DATA:String		= "NetStreamClientEvent/textData";		public static const XMP_DATA_ERROR:String	= "NetStreamClientEvent/error/xmpData";						////////////////////////////////////////////////////////////////////////////////////////////////////////////////
		// VARIABLES
		////////////////////////////////////////////////////////////////////////////////////////////////////////////////		public var data:Object;						////////////////////////////////////////////////////////////////////////////////////////////////////////////////
		// CONSTRUCTOR
		////////////////////////////////////////////////////////////////////////////////////////////////////////////////		public function NetStreamClientEvent(type:String, data:Object, bubbles:Boolean = false, cancelable:Boolean = false)		{			super(type, bubbles, cancelable);						this.data = data;		}				////////////////////////////////////////////////////////////////////////////////////////////////////////////////
		// CLONE
		////////////////////////////////////////////////////////////////////////////////////////////////////////////////		override public function clone():Event		{			return new NetStreamClientEvent(this.type, this.bubbles, this.cancelable);		}	}}